    "start": "node index.js",
    "develop": "node index.js ./keyboard.js 45 7",
    "build": "browserify public/app.js > public/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saibotsivad/node-raspberry-pi-skeeball.git"
  },
  "author": "saibotsivad",
  "license": "VOL",
  "bugs": {
    "url": "https://github.com/saibotsivad/node-raspberry-pi-skeeball/issues"
  },
  "homepage": "https://github.com/saibotsivad/node-raspberry-pi-skeeball#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "express": "^4.15.3",
    "jwerty": "^0.3.2",
    "keypress": "^0.2.1",
    "rpi-gpio": "^0.8.1",
    "socket.io": "^2.0.1"
  },
  "browserify": {
    "transform": [
      [
        "sveltify"
      ],
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "extensions": [
            ".js",
            ".html"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "sveltify": "^1.1.0"
